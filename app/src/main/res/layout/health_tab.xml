<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:background="#E0F2F4" android:id="@+id/col">
    <TextView
            android:text="90 mg/dl"
            android:layout_width="79dp"
            android:layout_height="25dp" android:id="@+id/textView5" tools:layout_editor_absoluteY="411dp"
            tools:layout_editor_absoluteX="72dp" android:textSize="14sp"/>
    <EditText
            android:layout_width="132dp"
            android:layout_height="38dp"
            android:inputType="textPersonName"
            android:text="Glucose Level"
            android:ems="10"
            android:id="@+id/editTextTextPersonName7" tools:layout_editor_absoluteY="470dp"
            tools:layout_editor_absoluteX="46dp" android:textAllCaps="false" android:textSize="12sp"
            android:fontFamily="sans-serif" android:textAppearance="@style/TextAppearance.AppCompat.Small"/>
    <ProgressBar
            style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Large"
            android:layout_width="105dp"
            android:layout_height="111dp"
            android:id="@+id/progressBar7" tools:layout_editor_absoluteY="368dp" tools:layout_editor_absoluteX="46dp"
            android:background="#0003DAC5"/>
    <com.google.android.material.tabs.TabLayout
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="50dp" android:id="@+id/Tab"
            app:tabTextAppearance="@style/TextAppearance.AppCompat.Body1" android:background="#3F51B5"
            app:tabIndicatorColor="#FDFDFD" app:tabTextColor="#FFFFFF" app:tabInlineLabel="true"
            android:clickable="true">

        <com.google.android.material.tabs.TabItem
                android:id="@+id/tab1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/basic_information"/>

        <com.google.android.material.tabs.TabItem
                android:id="@+id/tab2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Health"/>

        <com.google.android.material.tabs.TabItem
                android:id="@+id/tab3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Others"/>

    </com.google.android.material.tabs.TabLayout>
    <Spinner
            android:layout_width="409dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp" android:id="@+id/spinner3"
            tools:layout_editor_absoluteY="16dp"/>
    <TextView
            android:text="Patient List"
            android:layout_width="99dp"
            android:layout_height="17dp" android:id="@+id/textView" tools:layout_editor_absoluteY="16dp"
            android:background="#74E7F3F4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" android:autoText="true"/>
    <ProgressBar
            style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Large"
            android:layout_width="105dp"
            android:layout_height="111dp"
            android:id="@+id/progressBar4" tools:layout_editor_absoluteY="368dp" tools:layout_editor_absoluteX="240dp"
            android:background="#0003DAC5"/>
    <EditText
            android:layout_width="132dp"
            android:layout_height="38dp"
            android:inputType="textPersonName"
            android:text="Lactate Level"
            android:ems="10"
            android:id="@+id/editTextTextPersonName5" tools:layout_editor_absoluteY="470dp"
            tools:layout_editor_absoluteX="240dp" android:textAllCaps="false" android:textSize="12sp"
            android:fontFamily="sans-serif" android:textAppearance="@style/TextAppearance.AppCompat.Small"/>
    <TextView
            android:text="2.3 mmol/L"
            android:layout_width="79dp"
            android:layout_height="25dp" android:id="@+id/textView3" tools:layout_editor_absoluteY="411dp"
            tools:layout_editor_absoluteX="266dp" android:textSize="14sp"/>
    <Button
            android:text="Add notes"
            android:layout_width="408dp"
            android:layout_height="36dp" android:id="@+id/button5" tools:layout_editor_absoluteY="313dp"
            tools:layout_editor_absoluteX="2dp"/>
    <ProgressBar
            style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Large"
            android:layout_width="175dp"
            android:layout_height="147dp"
            android:id="@+id/progressBar2" tools:layout_editor_absoluteY="114dp" tools:layout_editor_absoluteX="118dp"
            android:background="#0003DAC5"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Overall Health Index"
            android:ems="10"
            android:id="@+id/editTextTextPersonName4" tools:layout_editor_absoluteY="249dp"
            tools:layout_editor_absoluteX="118dp" android:textAllCaps="false" android:textSize="18sp"
            android:fontFamily="sans-serif" android:textAppearance="@style/TextAppearance.AppCompat.Small"/>
    <TextView
            android:text="76"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2" tools:layout_editor_absoluteY="164dp"
            tools:layout_editor_absoluteX="191dp" android:textSize="36sp"/>
    <androidx.constraintlayout.widget.Group android:layout_width="wrap_content" android:layout_height="wrap_content"
                                            android:id="@+id/group"/>

</androidx.constraintlayout.widget.ConstraintLayout>

